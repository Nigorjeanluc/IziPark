{
  "name": "izi-parking",
  "version": "1.0.0",
  "description": "Backend for Izi parking project",
  "main": "index.js",
  "scripts": {
    "heroku-postbuild": "npm run build",
    "build": "babel server -d dist",
    "test": "npm run droptables:test && npm run createtables:test && npm run seed:test && cross-env NODE_ENV=test nyc --reporter=text --reporter=lcov mocha --require @babel/register --require babel-polyfill ./server/tests/*.js --exit",
    "dev": "nodemon --exec babel-node ./server/index.js",
    "createtables:pro": "cross-env NODE_ENV=production npx sequelize db:migrate",
    "seed:pro": "cross-env NODE_ENV=production npx sequelize db:seed:all",
    "droptables:pro": "cross-env NODE_ENV=production npx sequelize db:migrate:undo:all",
    "start": "npm run build && cross-env NODE_ENV=production babel-node ./dist/index.js",
    "createtables:dev": "npx sequelize db:migrate",
    "seed:dev": "npx sequelize db:seed:all",
    "droptables:dev": "npx sequelize db:migrate:undo:all",
    "createtables:test": "cross-env NODE_ENV=test npx sequelize db:migrate",
    "seed:test": "cross-env NODE_ENV=test npx sequelize db:seed:all",
    "coveralls": "npm test && nyc report --reporter=text-lcov | coveralls",
    "droptables:test": "cross-env NODE_ENV=test npx sequelize db:migrate:undo",
    "lint": "eslint ./server"
  },
  "author": "Igor Jean-Luc Ndiramiye",
  "license": "ISC",
  "dependencies": {
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@hapi/joi": "^17.1.0",
    "@sendgrid/mail": "^6.5.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-node": "0.0.1-security",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mailgen": "^2.0.10",
    "mocha": "^7.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.0.0",
    "pg": "^7.17.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.3",
    "swagger-ui-express": "^4.1.1"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/register": "^7.8.3",
    "babel-install": "2.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1"
  },
  "engines": {
    "npm": "6.4.1",
    "node": "10.13.0"
  }
}
